  "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.DS_Store": true,
       // "src/**/*.js": true,
        "**/*.js.map": true,
         "bower_components": true,
         "node_modules": true
  }
    }